# function for git log
function gl(){
count=-10
git log --graph --abbrev-commit --date=relative --decorate --pretty="format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset'" "$count" "$@"
}


